{
  "PlateServiceConfig": {
    //"BaseAddress": "http://127.0.0.1:5101/",
    "BaseAddress": "http://catalog-api/",
    "GetUrl": "catalog?pageNum={0}&pageSize={1}",
    "GetSortedUrl": "catalog?pageNum={0}&pageSize={1}&sortColumn={2}&sortDirection={3}&filter={4}",
    "PutUrl": "catalog/{0}",
    "Timeout": 30
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
